    "ax2.plot(o, np.abs(F), color='green');ax2.set_xlabel('$\\omega$');ax2.set_ylabel('$|X(i\\omega)|$'); ax2.set_title('$|X(i\\omega)|$')\n",
    "ax3.plot(o, np.angle(F), color='green');ax3.set_xlabel('$\\omega$');ax3.set_ylabel('Arg$(X(i\\omega))$'); ax3.set_title('Arg$(X(i\\omega))$')\n",
    "plt.show()"
   ]
