    "N = 60       # počet vzorků\n",
    "T = 1 / 10.0 # vzdálenost mezi vzorky\n",
    "t = np.linspace(-N*T/2., N*T/2., N)\n",
    "f = np.zeros(N)\n",
    "f[np.argwhere((t>-2) & (t < 2))] = 1\n",
